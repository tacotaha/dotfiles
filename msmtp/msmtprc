account taha_azzaoui@student.uml.edu
host smtp.office365.com
port 587
protocol smtp
auth on
from taha_azzaoui@student.uml.edu
user taha_azzaoui@student.uml.edu
passwordeval "pass show email/taha_azzaoui@student.uml.edu"
logfile ~/.msmtp.log
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account taha@tahaazzaoui.com
host smtp.fastmail.com 
port 587 
protocol smtp
auth on
from taha@tahaazzaoui.com 
user taha@tahaazzaoui.com 
passwordeval "pass show email/taha@tahaazzaoui.com"
logfile ~/.msmtp.log
tls on
tls_nocertcheck

account default : taha@tahaazzaoui.com 
