account "taha_azzaoui@student.uml.edu"
host smtp.office365.com
port 587
protocol smtp
auth on
from taha_azzaoui@student.uml.edu
user taha_azzaoui@student.uml.edu
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.passwd/taha_azzaoui@student.uml.edu.gpg"
logfile ~/.msmtp.log
tls on
tls_starttls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account "taha@tahaazzaoui.com"
host 127.0.0.1 
port 1143
from taha@tahaazzaoui.com 
user taha@tahaazzaoui.com 
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.passwd/taha@tahaazzaoui.com-onetime.gpg"
logfile ~/.msmtp.log
auth on
tls on
tls_starttls on
tls_fingerprint A3:1C:AB:7A:CD:C8:FF:02:43:0D:C4:E2:C8:09:E1:96:07:0C:21:E2 

